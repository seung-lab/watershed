keywords = {mathematical morphology,parallel implementation,quential algorithms,se-,watershed definition,watershed transform},
number = {1-2},
pages = {187--228},
title = {{The Watershed Transform: Definitions, Algorithms and Parallelization Strategies}},
volume = {41},
year = {2000}
}
@article{Couprie2003,
abstract = {We propose an original approach to the watershed problem, based on topology. We introduce a "one-dimensional" topology for grayscale images, and more generally for weighted graphs. This toplology allows us to precisely define a topological grayscale transformation that generalizes the action of a watershed transformation. Furthermore, we propose an efficient algorithm to compute this topological grayscale transformation, and we give an example of application to image segmentation.},
author = {Couprie, Michel and Bertrand, Gilles and Psi, Laboratoire and Esiee, Groupe},
doi = {10.1117/12.292778},
