keywords = {Boundary tracking,Distance transforms,Dljkstra's algorithm,Image analysis,Image segmentation,Morphological reconstruction,Multiscale skeletonization,Regional minima,Shortest-path problems,Watershed transform},
number = {1},
pages = {19--29},
